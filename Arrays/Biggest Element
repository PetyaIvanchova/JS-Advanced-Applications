function solve(input){
    let thebigest=Number.NEGATIVE_INFINITY;
 
    for (let i = 0; i < input.length; i++) {
       for (let j = 0; j <input[i].length; j++) {
        if(input[i][j]>=thebigest){
            thebigest=input[i][j];
        }
        
       }
        
        
    }
    return thebigest;
}
